{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*" : ["src/components/*"],
      "@hooks/*" : ["src/hooks/*"],
      "@lib/*" : ["src/lib/*"],
      "@utils/*" : ["src/utils/*"]
    },
    "types": ["@vitest/browser/matchers", "node"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
  },
  
}
